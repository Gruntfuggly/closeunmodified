{
    "name": "closeunmodified",
    "displayName": "Close Unmodified",
    "icon": "icon.png",
    "description": "Close files which aren't modified in git",
    "version": "0.0.4",
    "publisher": "Gruntfuggly",
    "repository": "https://github.com/Gruntfuggly/closeunmodified",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.closeUnmodifiedEditors",
                "title": "Close unmodified editors"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.closeUnmodifiedEditors",
                    "group": "close"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
